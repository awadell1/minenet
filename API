function open()
	local sides = {"left","right","front","back","top","bottom"}
	local modem = nil
	for (i=1,6,1) do
		if peripheral.isPresent(side[i]) then
			if (peripheral.getType(side[i]) == "modem") then
				modem = peripheral.wrap(side[i])
				if !modem.isWireless()
					modem = nil
			end
		end
	end
	if modem == nil
		return 0, "NoWirelessModemAttached"
	
